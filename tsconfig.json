{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "suppressImplicitAnyIndexErrors": true,
        "esModuleInterop": true
    },
    "typedocOptions": {
        "out": "docs",
        "excludeNotExported": true,
        "excludeProtected": true,
        "excludePrivate": true,
        "excludeExternals": true,
        "enrtyPoint": "app/pixi-console"
    }
}
